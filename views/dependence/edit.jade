.modal.fade#editDependenceModal(role="dialog")
    .modal-dialog
        .modal-content
            .modal-header
                button.close(type='button',data-dismiss='modal') x
                h3.modal-title Registrar Dependencia
            .modal-body
                form#form-dependence-edit
                    .row
                        .col-md-6
                            .form-group
                                label.control-label() Nombre
                                input#editNameDependence.form-control(type='text',name='nameDependence')
                                input#editIdDependence.form-control(type='hidden',name='id')
                        .col-md-6
                            .form-group
                                label.control-label() Siglas
                                input#editSiglasDependence.form-control(type='text',name='siglas')
                                    

                        .col-md-12
                            button#btnEditSubmitDependences_.btn.btn-success.pull-right(type='submit',style='margin-left:5px') Guardar
                            button.btn.btn-default.pull-right(type='button',data-dismiss='modal') Cancelar
